<?xml version="1.0" encoding="utf-8"?>
<skconfig>
	<archs>
		<!--Architecture to build-->
		<build>x86</build>
		<!--Supported Architecture-->
		<arch name="x86">
			<!--Build options,projects's private options can overide them-->
			<AS>gcc</AS>
			<ASFLAGS>-m32 -c</ASFLAGS>
			<CC>gcc</CC>
			<CFLAGS>-m32 -c</CFLAGS>
			<LD>ld</LD>
			<LDFLAGS></LDFLAGS>
			<CPP>gcc -M</CPP>
		</arch>
		<arch name="amd64">
			<AS>gcc</AS>
			<ASFLAGS>-c</ASFLAGS>
			<CC>gcc</CC>
			<CFLAGS>-c</CFLAGS>
			<LD>ld</LD>
			<LDFLAGS></LDFLAGS>
			<CPP>gcc -M</CPP>
		</arch>
	</archs>
	<targets>
		<target name="sanlo" build="true" src="src/sanlo"/>
		<target name="sandnix" build="true" src="src/sandnix"/>
		<target name="drivers" build="true" src="src/drivers"/>
	</targets>
</skconfig>
