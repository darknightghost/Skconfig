<?xml version="1.0" encoding="utf-8"?><skconfig name="Sandnix 0.0.2 Configuration">	
		<!--Architecture to build-->	
		<build actived="x86">		
				<!--Supported Architecture-->		
				<arch name="x86">			
						<!--Build options,projects's private options can override them-->			
						<AS type="textbox">gcc</AS>			
						<ASFLAGS type="textbox">-m32 -c</ASFLAGS>
						<ASRULE></ASRULE>
						<CC type="textbox">gcc</CC>	
						<CFLAGS type="textbox">-m32 -c</CFLAGS>
						<CCRULE></CCRULE>
						<LD type="textbox">ld</LD>
						<LDFLAGS type="textbox" />			
						<DEP type="textbox">gcc</DEP>
						<DEPRULE>-MM -o $@ $&lt;;echo `sed 's/^/$(OBJDIR)\/$(ARCH)\//' $@` &gt; $@</DEPRULE>
						<AFTER type="textbox" />			
				</arch>
				<arch name="amd64">			
						<AS type="textbox">gcc</AS>			
						<ASFLAGS type="textbox">-c</ASFLAGS>			
						<CC type="textbox">gcc</CC>			
						<CFLAGS type="textbox">-c</CFLAGS>			
						<LD type="textbox">ld</LD>			
						<LDFLAGS type="textbox" />			
						<DEP type="textbox">gcc</DEP>
						<DEPRULE>-MM -o $@ $&lt;;echo `sed 's/^/$(OBJDIR)\/$(ARCH)\//' $@` &gt; $@</DEPRULE>	
						<AFTER type="textbox" />			
				</arch>		
		</build>	
		<targets>		
				<target name="sanlo" src="src/sanlo" build="true" />		
				<target name="sandnix" src="src/sandnix" build="true" />		
				<target name="drivers" src="src/drivers" build="true" />		
		</targets>	
</skconfig>
