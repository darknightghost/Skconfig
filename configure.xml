<?xml version="1.0" encoding="utf-8"?><skconfig name="Sandnix 0.0.2 Configuration">	
	<!--Architecture to build-->	
	<build actived="x86">		
		<!--Supported Architecture-->		
		<arch name="x86">			
			<!--Build options,projects's private options can override them-->			
			<AS type="textbox">gcc</AS>			
			<ASFLAGS type="textbox">-m32 -c</ASFLAGS>
			<ASRULE type="textbox">aaaa</ASRULE>
			<CC type="textbox">gcc</CC>	
			<CFLAGS type="textbox">-m32 -c</CFLAGS>
			<CCRULE type="textbox"></CCRULE>
			<LD type="textbox">ld</LD>
			<LDFLAGS type="textbox"></LDFLAGS>
			<LDRULE type="textbox"></LDRULE>
			<DEP type="textbox">gcc</DEP>
			<DEPRULE type="textbox">-MM -o $@ $&lt;;echo `sed 's/^/$(OBJDIR)\/$(ARCH)\//' $@` &gt; $@</DEPRULE>
			<AFTER type="textbox"></AFTER>		
		</arch>
		<arch name="amd64">			
			<!--Build options,projects's private options can override them-->			
			<AS type="textbox">gcc</AS>			
			<ASFLAGS type="textbox">-m32 -c</ASFLAGS>
			<ASRULE type="textbox">@@@ &lt;</ASRULE>
			<CC type="textbox">gcc</CC>	
			<CFLAGS type="textbox">-m32 -c</CFLAGS>
			<CCRULE type="textbox"></CCRULE>
			<LD type="textbox">ld</LD>
			<LDFLAGS type="textbox"></LDFLAGS>		
			<LDRULE type="textbox"></LDRULE>
			<DEP type="textbox">gcc</DEP>
			<DEPRULE type="textbox">-MM -o $@ $&lt;;echo `sed 's/^/$(OBJDIR)\/$(ARCH)\//' $@` &gt; $@</DEPRULE>
			<AFTER type="textbox"></AFTER>
		</arch>		
	</build>	
	<targets>		
		<target build="true" name="sanlo" src="src/sanlo"/>		
		<target build="true" name="sandnix" src="src/sandnix"/>		
		<target build="true" name="drivers" src="src/drivers"/>		
	</targets>	
</skconfig>