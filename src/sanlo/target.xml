<?xml version="1.0" encoding="utf-8"?><target>
	<build objdir="" output="">
		<arch name="amd64">
			<!--Build options,projects's private options can override them-->			
			<AS type="textbox">gcc</AS>			
			<ASFLAGS type="textbox">-m32 -c</ASFLAGS>
			<ASRULE type="textbox"></ASRULE>
			<CC type="textbox">gcc</CC>	
			<CFLAGS type="textbox">-m32 -c</CFLAGS>
			<CCRULE type="textbox"></CCRULE>
			<LD type="textbox">ld</LD>
			<LDFLAGS type="textbox"></LDFLAGS>
			<LDRULE type="textbox"></LDRULE>		
			<DEP type="textbox">gcc</DEP>
			<DEPRULE type="textbox">-MM -o $@ $&lt;;echo `sed 's/^/$(OBJDIR)\/$(ARCH)\//' $@` &gt; $@</DEPRULE>
			<AFTER type="textbox"></AFTER>			
		</arch>
	</build>
	<menu>
		<options text="sanlo menu1">
			<option text="group1" type="lable"/>
			<option macro="-Daaa=" text="list1" type="listcontrol" value="2">
				aaa
				bbb
				ccc
			</option>
			<option text="group2" type="lable"/>
			<option macro="-DbbB" text="chk1" type="checkbox" value="false"/>
			<option macro="-Dccc" text="chk2" type="checkbox" value="true"/>
		</options>
		<options text="test menu2">
		</options>
	</menu>
</target>